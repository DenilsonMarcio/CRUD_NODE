<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>My CRUD in NodeJS</title>
</head>
<body>

<h2>Registros<h2>
    <table border="0">
        <thead>
            <tr>
                <td>CPF</td>
                <td>Nome</td>
                <td>Sobrenome</td>
            </tr>
        </thead>
        <tbody>
        <% data.forEach(function(details) { %>
            <tr>
                <td><%= details.cpf %></td>
                <td><%= details.name %></td>
                <td><%= details.surname %></td>
                <td><a href="/edit/<%= details.cpf %>">Editar</a> - <a href="/delete/<%= details.cpf %>">Deletar</a></td>
            </tr>
            <% }) %>
        </tbody>
    </table>
        <button><a href="/">Novo</a></button>
</body>
</html>